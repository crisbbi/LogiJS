  .edu-bg(style='background-image: url(\'' + background + '\');')
  .top-menu#top
    .container
      .navbar.navbar-default.navbar-fixed-top(role='navigation', style='background: none;')
        .container
          .navbar-header
            a.navbar-brand(href='#', style="transition: 0.2s;")
              img#top_logo(src='images/logo_index_new.png', alt='LogiJS logo', title='LogiJS', style='float: left;')
              if show_org_name
                div#logo-div(style="float: right; margin: 0; padding: 0; border-left: 5px solid #c83232; padding-left: 20px;")
                  p#logo-text #{org_first_line}
                    br
                    | #{org_second_line}
                    span(style="color: #c83232;") .
          .navbar-collapse.collapse
            ul.nav.navbar-nav.navbar-right
              li
                a.internal-link(rel='nofollow', href='#', target='_parent') <i class="fa fa-sign-in-alt" style="color: #c83232;"></i> #{localization.login}
              if show_registration
                li
                  a.internal-link(rel='nofollow', href='/signup', target='_parent') <i class="fa fa-user-plus" style="color: #c83232;"></i> #{localization.register}
              li
                a#mode-button.internal-link(rel='nofollow', href='javascript:;', target='_parent' title=localization.toggle_dark_mode) <i class="fa fa-moon red"></i>
  .slogan-container(style="margin-top: 150px;")
    h1.background-text #{background_text}
  .login_container
    form.Login.login-background
      if failed
        p.login-message #{localization.login_failed}
          span(style="color: #c83232;") .
      else if signup_success
        p.login-message #{localization.signup_successful}
          span(style="color: #c83232;") .
      else
        h1.login_title #{localization.login}
          span(style="color: #c83232;") .
      p
        input.username(type='text', placeholder=localization.username)
        br
        input.password(type='password', placeholder=localization.password)
        br
        input.btn.btn-lg.btn-orange.login-confirm-button(type='submit', value=localization.login, role='button')
      p.login-redirect-text
        if allow_enter
          a.login-redirect-text(href='/editor') #{localization.continue_without_login}
      br
  .footer_bottom.footer_bottom_absolute
      .footer_bottom_content
        .footer_box.footer_box_outer
          if refer_to_logijs_com
            a(rel='nofollow', href='https://logijs.com/', target='_blank') <i class="fa fa-external-link-alt" style="color: #c83232;"></i> #{localization.switch_to_logijs_com}
        .footer_box.footer_box_inner
        .footer_box.footer_box_outer.footer_right
          if made_in_luebeck
            p(style="text-align: right;") Made with <i class="fas fa-heart" style="color: #c83232;"></i> in LÃ¼beck
              span(style="color: #c83232;") .
